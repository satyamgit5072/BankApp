pipeline{
    agent any
    stages{
        stage('Compile Stage'){
            steps{
             withMaven(maven: 'maven 3'){
                sh 'mvn clean compile'
                }
            }
        }
    stage('Test Stage'){
            steps{
             withMaven(maven: 'maven 3'){
                sh 'mvn test'
                }
            }
        }
    stage('Build Stage'){
            steps{
             withMaven(maven: 'maven 3'){
                sh 'mvn build'
                }
            }
        }
    }

 

}